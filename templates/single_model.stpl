<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags for bootstrap -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  </head>
  <body>
    <main class="container">
      <div class="row">
        <h1><%= benchmark_name %>: <%= op %></h1>
      </div>
      <hr>

      <div class="row">
        <h4>Linear Model: <%= linear_model.a%>*x+<%= linear_model.b%></h4>
        <aside>Plotted logarithmically (log2)</aside>
        <%- linear_model_svg %>
      </div>

      <% for (json, kde) in jsons.iter().zip(kdes.iter()) { %>
      <div class="row">
        <h4>
          Access Size: <%= json.access_size_in_bytes %>
        </h4>
      </div>
      <div class="center" />

      <%# This is unescaped %>
      <%- kde.to_svg() %>

      <% } %>
      </div>

    </main>
  </body>
</html>
